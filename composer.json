{
    "name": "orchestra/debug",
    "description": "Artisan Debug Profiler for Laravel",
    "homepage": "http://orchestraplatform.com/docs/latest/components/debug/",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "profiler", "debug"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Debug\\" : "src/"
        }
    },
    "require": {
        "php": ">=7.0",
        "illuminate/console": "~5.5.0",
        "orchestra/support": "~3.5.0",
        "react/socket": "0.5.*"
    },
    "require-dev": {
        "illuminate/container": "~5.5.0",
        "illuminate/database": "~5.5.0",
        "mockery/mockery": "~0.9.4",
        "monolog/monolog": "~1.11"
    },
    "suggest": {
        "laravel/framework": "Allow using orchestra/debug as added feature for Laravel (~5.4)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.5-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
